{
    "name": "@openkite/kite-ui",
    "private": false,
    "version": "0.0.1-experimental",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:lib": "vite build --mode lib",
        "preview": "vite preview",
        "prepublishOnly": "npm run build:lib",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "release:patch": "npm version patch && git push origin --tags",
        "release:minor": "npm version minor && git push origin --tags",
        "release:major": "npm version major && git push origin --tags",
        "publish:dev": "npm run build:lib && npm publish --tag dev"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwind-variants": "^0.3.0"
    },
    "devDependencies": {
        "@storybook/addon-docs": "^9.1.1",
        "@storybook/addon-onboarding": "^9.1.1",
        "@storybook/react-vite": "^9.1.1",
        "@tailwindcss/vite": "^4.0.0-alpha.37",
        "@types/node": "^22.10.5",
        "@types/react": "^18.3.17",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react": "^4.3.4",
        "prettier": "^3.6.2",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "storybook": "^9.1.1",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "^4.0.0-alpha.37",
        "typescript": "~5.6.2",
        "vite": "^6.0.5",
        "vite-plugin-dts": "^4.3.0"
    }
}
